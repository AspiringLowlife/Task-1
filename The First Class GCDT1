 public class GCDT1
    { Stopwatch Timer = new Stopwatch();           
        //Finds the GCD of two integer arguments
        public int FindGCD(int A, int B)     
        {
            
            Timer.Start();     
            List<int> DivisorList = FindDivisors(A);
            for (int i = DivisorList.Count - 1; i >= 0; i--)
            {
                if (B % DivisorList[i] == 0)
                    return DivisorList[i];               
            }
            return 0;
            Timer.Stop();
            
        }
        //Finds the Divisors of the integer A and fills an int List with each Divisor 
        public List<int> FindDivisors(int A)
        {
            List<int> List = new List<int>();
            for (int i = 1; i <= A; i++)
            {
                if (A % i == 0)
                    List.Add(i);           //Adds i as a divisor of A to the List ensuring the list is in ascending order
            }
            return List;
        }   
    }
